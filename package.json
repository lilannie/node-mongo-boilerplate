{
  "name": "mongo-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Mongo Node API",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && nodemon dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lilannie/node-mongo-boilerplate.git"
  },
