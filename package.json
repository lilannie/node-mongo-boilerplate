  "bugs": {
    "url": "https://github.com/lilannie/node-mongo-boilerplate/issues"
  },
  "homepage": "https://github.com/lilannie/node-mongo-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.0.1",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "mongoose": "^4.11.7",
    "morgan": "^1.8.2"
  }
